            </Field>
            <Field id="port" type="textfield" defaultValue="80" tooltip="Wunderground Server Port">
                <Label>Wunderground Server Port:</Label>
            </Field>
        </ConfigUI> 
        <States>
            <State id="status">
                <ValueType>String</ValueType>
                <TriggerLabel>Status</TriggerLabel>
