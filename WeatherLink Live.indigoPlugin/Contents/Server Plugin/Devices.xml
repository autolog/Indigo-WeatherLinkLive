            </Field>            
            <Field id="baro_device" type="menu">
                <Label>Barometer Device:</Label>
                <List class="self" filter="" method="baroDeviceList"/>
            </Field>            
            <Field id="baro_state" type="menu" defaultValue="bar_sea_level">
                <Label>Barometer value to send:</Label>
